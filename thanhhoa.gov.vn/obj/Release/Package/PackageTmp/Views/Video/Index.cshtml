@{
    ViewBag.Title = "Index";
    ViewBag.Title = "Danh sách video";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var lstVideo = (List<thanhhoa.gov.vn.Models.gov_video>)ViewData["lstVideo"];
}
<div class="mainMod">
    <div class="modLeftMain">
        <div class="breackrum">
            <a class="bkHome" href="index.html">Trang chủ</a>
            <a href="5.1video.html">Video</a>
        </div>
        <!--end breackrum-->

        <div class="modMulti">

